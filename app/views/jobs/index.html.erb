<div class="row">
  <table class="table table-striped">
    <thead>
      <th>工作名称</th>
      <th>工作地点</th>
      <th>工资下限</th>
      <th>工资上限</th>
      <th>发布时间</th>
    </thead>
    <tbody>
      <% @jobs.each do |job| %>
        <tr>
          <td><%= link_to(job.title, job_path(job)) %></td>
          <td><%= job.location %></td>
          <td><%= job.wage_lower_bound %></td>
          <td><%= job.wage_upper_bound %></td>
          <td><%= render_job_time(job) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
